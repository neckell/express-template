{
  "name": "express-template",
  "description": "This is a project to serve as a nodejs template server using Express and MongoDB",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\""
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.18.2",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "mongodb": "^4.4.0",
    "mongoose": "^8.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1"
  },
  "author": "Nicolas Eckell",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neckell/express-template.git"
  },
  "keywords": [
    "template",
    "hexagonal",
    "nodejs",
    "apirest"
  ],
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.3.3"
  }
}
